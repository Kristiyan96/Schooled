$("tr.<%=@student.id%>").after("<%= escape_javascript(render 'courses/student_row') %>").remove();

$(function() {
    $('.give-marks a.new').click(function(){
    	mark  = $(this).html();
    	$(this).parent().parent().siblings('.mark_mark')[0].firstChild.value=mark;
    	var form = $(this).parent().parent().parent();
    	form.submit();
    });
});